@model Result
@{
    ViewData["Title"] = "Result";
}

<table class="table table-bordered">
      <tr>
          <th>Controller: </th>
          <td>
              @Model.Controller
          </td>
      </tr>
      <tr>
          <th>Action: </th>
          <td>
              @Model.Action
          </td>
      </tr>
          @foreach(string key in Model.Data.Keys)
          {
              <tr>
                  <th>@key:  </th>
                  <td>
                      @Model.Data[key]
                  </td>
              </tr>
          }
      
</table>
